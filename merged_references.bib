@article{Wang2002,
	Abstract = {A point interpolation meshless method is proposed based on combining radial and polynomial basis functions. Involvement of radial basis functions overcomes possible singularity associated with the meshless methods based on only the polynomial basis. This non-singularity is useful in constructing well-performed shape functions. Furthermore, the interpolation function obtained passes through all scattered points in an influence domain and thus shape functions are of delta function property. This makes the implementation of essential boundary conditions much easier than the meshless methods based on the moving least-squares approximation. In addition, the partial derivatives of shape functions are easily obtained, thus improving computational efficiency. Examples on curve=surface fittings and solid mechanics problems show that the accuracy and convergence rate of the present method is high},
	Author = {Wang, J. G. and Liu, G. R.},
